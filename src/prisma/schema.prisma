generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model GameEvent {
  id        String   @id @default(uuid())
  type      String
  data      Json
  createdAt DateTime @default(now())
}